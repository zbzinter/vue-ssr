    "ssrDev": "cross-env RUN_MODE=hot node server",
    "ssrRestart:pm2": "pm2 stop qiuhuiSSR && pm2 start server/pm2.json",
    "ssrStart:pm2": "npm run pm2Log && pm2 start server/pm2.json",
    "ssrStop": "pm2 stop qiuhuiSSR",
